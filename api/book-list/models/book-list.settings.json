{
  "connection": "default",
  "collectionName": "book_lists",
  "info": {
    "name": "Book List"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "bookList": {
      "type": "component",
      "repeatable": false,
      "component": "lists.resource-list"
    },
    "books": {
      "via": "book_list",
      "collection": "books"
    }
  }
}
